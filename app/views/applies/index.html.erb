<table>
    <tr>
        <th>Id</th>
        <th>Titulo</th>
        <th>Nombre del solicitante</th>
        <th>Solicitud</th>
        <th>Edad</th>
    </tr>
    <% @applies.each do |a| %>
        <tr>
        <th><%=a.id%></th>
        <th><%=a.title%></th>
        <th><%=a.name%></th>
        <th><%=a.body%></th>
        <th><%=a.edad%></th>
    </tr>
    <%end%>
</table>

<div>
    <h1> Datos del modelo:</h1>
    <p><%= @applies.inspect %></p>
</div>